{
  "name": "ng-jsonschema",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "build:fullapp": "npm run build:app && rimraf src/ng-jsonschema",
    "build:lib": "rimraf dist && ng-packagr -p ng-jsonschema/package.json",
    "build:sass": "npm rebuild node-sass",
    "publish-npm": "npm run build:lib && cd dist/ng-jsonschema && npm publish",
    "test": "npm run build:lib && npm run build:fullapp && npm run e2e",
    "copy": "mkdir -p src/ng-jsonschema/ && cp -R dist/ng-jsonschema/* src/ng-jsonschema",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "dependencies": {
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "*",
    "@angular/forms": "*",
    "@angular/http": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/router": "*",
    "core-js": "^3.6.4",
    "rxjs": "*",
    "webpack": "*",
    "ang-jsoneditor": ">=1.9.1",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.11",
    "@angular-devkit/schematics": "^0.6.0",
    "@angular/cli": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@types/jasmine": "3.3.8",
    "@types/node": "~10.1.0",
    "codelyzer": "~5.0.0",
    "cpr": "^3.0.0",
    "jasmine": "^3.3.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^3.0.6",
    "protractor": "~5.4.0",
    "rimraf": "^2.7.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.29.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "typescript": "3.4.3"
  }
}
